    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "electron-forge": "^5.2.4",
    "electron-webpack": "^2.8.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.0",
